<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.AdminLoginCtrl">
   <children>
      <Button fx:id="backButton" layoutX="27.0" layoutY="91.0" mnemonicParsing="false" onAction="#back" prefHeight="31.0" prefWidth="92.0" text="Back">
         <font>
            <Font size="14.0" />
         </font></Button>
      <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="44.0" prefHeight="30.0" prefWidth="200.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font></PasswordField>
      <Button fx:id="loginButton" layoutX="281.0" layoutY="91.0" mnemonicParsing="false" onAction="#validate" prefHeight="30.0" prefWidth="92.0" text="Login">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
