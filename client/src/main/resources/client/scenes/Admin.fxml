<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.AdminCtrl">
   <children>
      <ListView fx:id="listView" layoutX="85.0" layoutY="73.0" prefHeight="254.0" prefWidth="430.0" />
      <Button fx:id="backButton" layoutX="23.0" layoutY="353.0" mnemonicParsing="false" onAction="#back" text="Back">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Text fx:id="adminText" layoutX="23.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin">
         <font>
            <Font name="System Italic" size="24.0" />
         </font>
      </Text>
      <Button fx:id="importButton" layoutX="272.0" layoutY="338.0" mnemonicParsing="false" onAction="#importBackup" prefHeight="30.0" prefWidth="98.0" text="Import">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="downloadSelectedButton" layoutX="127.0" layoutY="338.0" mnemonicParsing="false" onAction="#downloadSelected" text="Download Selected">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="selectAllButton" layoutX="376.0" layoutY="337.0" mnemonicParsing="false" onAction="#selectAll" prefHeight="30.0" prefWidth="97.0" text="Select all">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <ComboBox fx:id="sortComboBox" editable="true" layoutX="212.0" layoutY="39.0" prefWidth="150.0" promptText="Sort by" />
   </children>
</AnchorPane>
