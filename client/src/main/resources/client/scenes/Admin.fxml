<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.AdminCtrl">
   <children>
      <ListView fx:id="listView" layoutX="85.0" layoutY="73.0" onMouseClicked="#handleEventClick" prefHeight="254.0" prefWidth="430.0" />
      <Button fx:id="backButton" layoutX="23.0" layoutY="353.0" mnemonicParsing="false" onAction="#back" text="Back">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="lastActivityButton" layoutX="145.0" layoutY="30.0" mnemonicParsing="false" onAction="#sortByLastActivity" prefHeight="30.0" prefWidth="112.0" text="Last Activity">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="titleButton" layoutX="262.0" layoutY="31.0" mnemonicParsing="false" onAction="#sortByTitle" prefHeight="30.0" prefWidth="62.0" text="Title">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="creationDateButton" layoutX="331.0" layoutY="31.0" mnemonicParsing="false" onAction="#sortByCreationDate" prefHeight="30.0" prefWidth="123.0" text="Creation date">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Text fx:id="adminText" layoutX="23.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin">
         <font>
            <Font name="System Italic" size="24.0" />
         </font>
      </Text>
      <Button fx:id="importButton" layoutX="288.0" layoutY="338.0" mnemonicParsing="false" text="Import">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="downloadSelectedButton" layoutX="145.0" layoutY="338.0" mnemonicParsing="false" text="Download Selected">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="selectAllButton" layoutX="356.0" layoutY="338.0" mnemonicParsing="false" text="Select all">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
