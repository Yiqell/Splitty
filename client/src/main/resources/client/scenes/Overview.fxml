        </Text>
        <Button fx:id="sendinvitesButton" layoutX="466.0" layoutY="39.0" mnemonicParsing="false" onAction="#sendInvites" text="Send Invites">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Text fx:id="participantsText" layoutX="27.0" layoutY="173.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Participants" wrappingWidth="106.39999999999986">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <Button fx:id="editParticipantsButton" layoutX="147.0" layoutY="151.0" mnemonicParsing="false" onAction="#editParticipants" text="Edit">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="addParticipantsButton" layoutX="200.0" layoutY="151.0" mnemonicParsing="false" onAction="#addParticipant" prefHeight="30.0" prefWidth="50.0" text="Add">
