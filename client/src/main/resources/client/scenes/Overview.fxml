      </Button>
      <Button fx:id="fromButton" layoutX="126.0" layoutY="350.0" mnemonicParsing="false" onAction="#showFromPersonExpenses" prefHeight="30.0" prefWidth="200.0" text="From">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="includingButton" layoutX="333.0" layoutY="350.0" mnemonicParsing="false" onAction="#showIncludingPersonExpenses" prefHeight="30.0" prefWidth="200.0" text="Including">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="41.0" layoutY="643.0" mnemonicParsing="false" onAction="#back" text="Back" />
